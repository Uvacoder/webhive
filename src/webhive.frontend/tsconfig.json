{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "strict": true,
    "esModuleInterop": false,
    "noImplicitAny": false,
    "experimentalDecorators": false,
    "emitDecoratorMetadata": false,
    "outDir": "dist",
    "pretty": true,
    "removeComments": false,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": false,
    "skipLibCheck": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": [
      "dom",
      "esnext"
    ]
  },
  "include": [
    "app.component/**/*.ts",
    "app.component/**/*.tsx"
  ]
}
